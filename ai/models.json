{
  "models": [
    {
      "id": "whale_predictor",
      "name": "Whale Movement Predictor",
      "type": "regression",
      "description": "Predicts whale intent based on transaction history.",
      "training_script": "training/train_whale_model.py",
      "inference_function": "inferWhaleIntent",
      "params": {
        "input_features": ["tx_volume", "wallet_age", "liquidity_flow"],
        "output": "intent_score (0-1)",
        "layers": [64, 32, 1]
      },
      "onchain_export": "onchain/AIParams.sol:WHALE_WEIGHTS"
    },
    {
      "id": "risk_scorer",
      "name": "Predictive Risk Scorer",
      "type": "classification",
      "description": "Scores trade risks using market regimes.",
      "training_script": "training/train_risk_model.py",  
      "inference_function": "inferRiskScore",
      "params": {
        "input_features": ["volatility", "volume_spike", "regime_type"],
        "output": "risk_level (low/medium/high)",
        "layers": [128, 64, 3]
      },
      "onchain_export": "onchain/AIParams.sol:RISK_THRESHOLDS"
    }
  ],
  "version": "1.0",
  "last_updated": "2026-02-06"
